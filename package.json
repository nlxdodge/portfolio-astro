{
  "name": "portfolio-astro",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build && npx pagefind --site dist && cp -r dist/pagefind public/",
    "build-win": "astro build && npx pagefind --site dist && copy /dist/pagefind /public",
    "format": "prettier --write .",
    "preview": "pnpm i && pnpm run build && astro preview",
    "astro": "astro"
  },
  "comments": {
    "comment": "astro-purgecss is stuck on version 4.5.0 for now as higher version break the css hashes during the build time"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/mdx": "^3.1.9",
    "@astrojs/partytown": "^2.1.2",
    "@astrojs/rss": "^4.0.9",
    "@astrojs/sitemap": "^3.2.1",
    "@glidejs/glide": "^3.6.2",
    "@iconify-json/arcticons": "^1.2.4",
    "@iconify-json/eva": "^1.2.0",
    "@iconify-json/mdi": "^1.2.1",
    "@iconify-json/ph": "^1.2.1",
    "@iconify-json/ri": "^1.2.3",
    "@iconify-json/simple-icons": "^1.2.11",
    "@playform/compress": "^0.1.6",
    "astro": "^4.16.10",
    "astro-icon": "^1.1.2",
    "astro-purgecss": "^4.6.2",
    "astro-robots-txt": "^1.0.0",
    "astro-seo": "^0.8.4",
    "include-media": "^2.0.0",
    "remark-toc": "^9.0.0",
    "sharp": "^0.33.5",
    "typescript": "^5.6.3"
  },
  "devDependencies": {
    "pagefind": "^1.2.0",
    "prettier": "^3.3.3",
    "prettier-plugin-astro": "^0.14.1",
    "rehype-accessible-emojis": "^0.3.2",
    "sass": "^1.80.6",
    "svgo": "^3.3.2"
  },
  "packageManager": "pnpm@9.12.2"
}
